-- start_matchsubs

m/^ERROR: .* (seg\d+ [0-9.]+:\d+ pid=\d+)/
s/seg\d+ [0-9.]+:\d+ pid=\d+/SEG IP:PORT pid=PID/

# skip specific PID in: ERROR:  end point token_id3 already attached by receiver(pid: 50938) (cdbendpoint.c:XXX)
m/^ERROR:.*\(pid: \d+\) \(cdbendpoint.c:\d+\)/
s/\(pid: \d+\) \(cdbendpoint.c:\d+\)/\(pid: PID\) \(cdbendpoint.c:LINENO\)/

# skip token id in: ERROR:  can't add a new token tk0618798867 into shared memory (cdbendpoint.c:LINENO)
m/^ERROR:.*a new token tk\d+.*\(cdbendpoint.c:\d+\)/
s/a new token tk\d+ (.*)\(cdbendpoint.c:\d+\)/a new token token_id_no $1\(cdbendpoint.c:LINENO\)/

m/^ERROR:.*\(cdbendpoint.c:\d+\)/
s/\(cdbendpoint.c:\d+\)/\(cdbendpoint.c:LINENO\)/


-- end_matchsubs
